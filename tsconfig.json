{
  "compilerOptions": {
    "alwaysStrict": true,
    "esModuleInterop": true,
    "lib": [
      "es2018",
      "webworker"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2018",
    "types": [
      "@cloudflare/workers-types",
      "@types/node"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "test"
  ],
  "include": [
    "src"
  ]
}
